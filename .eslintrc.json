{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "NODE_ENV": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off"
    }
}
